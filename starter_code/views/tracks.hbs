<nav class="navbar bg-dark">
  <span>Title</span>
  <span>Listen</span>
</nav>
 
 <div class="row">
  {{#each items as |track|}} 
  <div class="card w-100" style="background-color: #E0E2E5;">
        <div class="card-body" style="padding: 2%;">
          <h1 class="track-info">{{track.name}}</h1> 
          <audio controls src="{{preview_url}}" type="audio/mp3"></audio>        
        </div>
      </div>    
  </div>
  {{/each}}
</div> 